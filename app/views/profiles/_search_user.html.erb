<%= search_form_for @profile_q do |f| %>
  <div class="container_search">
  <h3>【ユーザー検索】</h3>
    <div class="row">
      <div class="col"></div>
      <div class="col">
        <%= f.label :prefecture %>
      </div>
      <div class="col">
        <%= f.select :prefecture_eq, Profile.prefectures.keys.map, include_blank:'選択してください', class:"form-control" %>
      </div>
      <div class="col"></div>
    </div>
    <div class="row">
      <div class="col"></div>
      <div class="col">
        <%= f.label :main_crop %>
      </div>
      <div class="col">
        <%= f.search_field :main_crop_cont, placeholder: '生産作物で検索', class:"form-control"%>
      </div>
      <div class="col"></div>
    </div>
    <div class="row">
      <div class="col"></div>
      <div class="col">
        <%= f.label :drone_name %>
      </div>
      <div class="col">
        <%= f.search_field :drones_drone_name_cont, placeholder: '機体名で検索', class:"form-control" %>
      </div>
      <div class="col"></div>
    </div>
    <div class="buttom_search">
      <%= f.submit " 検索 ", class:"btn btn-outline-dark" %>
      <%= button_tag 'リセット', type: 'reset', class:"btn btn-outline-warning" %>
    </div>
  </div>
<% end %>