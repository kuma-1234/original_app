<%= search_form_for @profile_q do |f| %>
  <div class="container_search">
  <h5>【ユーザー検索】</h5>
    <div class="mb3">
      <%= f.label :prefecture, class:"form-label" %>
      <%= f.select :prefecture_eq, Profile.prefectures.keys.map, include_blank:'選択してください', class:"form-control" %>
    </div>
    <div class="mb3">
      <%= f.label :main_crop, class:"form-label" %>
      <%= f.search_field :main_crop_cont, placeholder: '生産作物で検索', class:"form-control"%>
    </div>
    <div class="mb3">
      <%= f.label :drone_name, class:"form-label"  %>
      <%= f.search_field :drones_drone_name_cont, placeholder: '機体名で検索', class:"form-control" %>
    </div>
    <div class="buttom_search">
      <div class="row">
        <div class="col"><%= f.submit " 検索 ", class:"btn btn-outline-dark w-100" %></div>
        <div class="col"><%= button_tag 'リセット', type: 'reset', class:"btn btn-outline-warning w-100" %></div>
      </div>
    </div>
  </div>
<% end %>